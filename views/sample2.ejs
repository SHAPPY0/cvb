<%- include header.ejs%>
<style>

.marign-50{margin-top:50px; margin-bottom:50px}
.ma-30{margin-top:30px; margin-bottom:30px !important}
.border{border:1px solid #ccc; margin:30px auto; width:70%;  padding:20px 25px;}
.container_details{float:right}
</style>
<div class="container">
<div id="buttons"></div>

<br />
	<div id="JSFiddle">
	<table style="width:70%; border:1px solid #ccc; padding:40px 10px 40px 10px">
<tr>
	<td>
		<p style="text-align:center; font-size:25px; font-weight:bold; text-transform:uppercase"><%=data.personaldetails.candidatename%></p>
		<p style="text-align:center; font-size:15px; color:#333; margin:5px 0px !important"><%=data.personaldetails.permanentaddress%>,<%=data.personaldetails.permanentcity%>, <%=data.personaldetails.permanentstate%></p>
        <p style="text-align:center; font-size:15px; color:#333; margin:5px 0px !important"><span><b>Mobile:&nbsp;</b>+91-<%=data.personaldetails.mobile%></span>&nbsp;&nbsp;<span><b>Email:	&nbsp;</b><a href="mailto:<%=data.personaldetails.email%>"><%=data.personaldetails.email%></a></span></p>
        <p style="height:2px; background:#333"></p>
	</td> 
</tr>
<tr>
	<td>
    <h4 style="margin:20px 0px 6px 0px !important;background: #f1f1f1; padding:5px"><b>Objectives:</b></h4>
    <p style="font-size:14px"><%=data.cv.objective%></p>
    </td>
</tr>
<tr>
	<td>
    <h4 style="margin:20px 0px 15px 0px !important;background: #f1f1f1; padding:5px"><b>Educational Details:</b></h4>
   <div style="margin-bottom:20px">
   <p style="margin-bottom:10px; font-weight:700"><%=data.educationaldetails.highschool.course%><span style="float:right">[<%=data.educationaldetails.highschool.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.highschool.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</span></p>
   <p style="font-size:13px;"><%=data.educationaldetails.highschool.school%></p> 
   </div>
   
    <div style="margin-bottom:20px !important">
   <p style="margin-bottom:10px; font-weight:700"><%=data.educationaldetails.seniorsecondaryschool.course%><span style="float:right">[<%=data.educationaldetails.seniorsecondaryschool.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.seniorsecondaryschool.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</span></p>
   <p style="font-size:13px;"><%=data.educationaldetails.seniorsecondaryschool.school%></p> 
   </div>
   
    <div style="margin-bottom:20px !important">
   <p style="margin-bottom:10px; font-weight:700"><%=data.educationaldetails.undergraduation.course%><span style="float:right">[<%=data.educationaldetails.undergraduation.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.undergraduation.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</span></p>
   <p style="font-size:13px;"><%=data.educationaldetails.undergraduation.university%></p> 
   </div>
    <div style="margin-bottom:20px !important">
   <p style="margin-bottom:10px; font-weight:700"><%=data.educationaldetails.postgraduation.course%><span style="float:right">[<%=data.educationaldetails.postgraduation.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.postgraduation.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</span></p>
   <p style="font-size:13px;"><%=data.educationaldetails.postgraduation.university%></p> 
   </div>
    </td>
</tr>
<tr>
	<td>
    <h4 style="margin:20px 0px 15px 0px !important;background: #f1f1f1; padding:5px"><b>Work Experience:</b></h4>
    <div style="margin-bottom:20px">
    <p style=" font-weight:700"><%=data.professionaldetails.company.companyname%><span style="float:right">[<%=data.professionaldetails.company.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.professionaldetails.company.enddate%>]</span></p>
    <p style="margin-bottom:10px;"><%=data.professionaldetails.company.jobtitle%></p>
    <p style="font-size:14px;"><%=data.professionaldetails.company.roles%></p> 
    </div>
   
    </td>
</tr>
<tr>
	<td>
    <h4 style="margin:20px 0px 15px 0px;background: #f1f1f1; padding:5px"><b>Skills:</b></h4>
    <div style="font-size:15px; margin-bottom:5px !important;"><p><b>Primary Skill:</b><%=data.skills.primaryskill%></p></div>
     <div style="font-size:15px"><p><b>Secondary Skill:</b><%=data.skills.secondaryskill%></p></div>
    </td>
</tr>
</table>

</div>
</div>
<%- include footer.ejs%>
<script src="/javascripts/jquery.min.js"></script>
<script src="/javascripts/xeponline.jqplugin.js"></script>
<script>
var click="return xepOnline.Formatter.Format('JSFiddle', {render:'download'})";
jQuery('#buttons').append('<span onclick="'+ click +'" class="btnn" ><img src="data:image/png;base64," width="20px"></span>');
</script>
<style>
table{border-collapse:inherit !important}
.btnn{ background:#000; padding:5px; cursor:pointer ;  right: 416px;
    top: 90px; position:absolute;  }
	
</style>
