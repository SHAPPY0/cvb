<%- include header.ejs%>
<link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700,800" rel="stylesheet">
<style>

table{font-family:'Open Sans', sans-serif; font-size:13px !important}
h4,h3, h2, h1 p{padding:0px !important; margin:0px !important}
.marign-50{margin-top:50px; margin-bottom:50px}
.ma-30{margin-top:30px; margin-bottom:30px}
.border{border:1px solid #ccc; margin:30px auto; width:70%;  padding:20px 25px;}
.container_details{float:right}
</style>
<div class="container">
<div id="buttons"></div>

<br />
	<div id="JSFiddle">
	
<table style="width:70%; border:1px solid #ccc; padding:40px 20px 40px 20px; font-size:14px">
<tr>
	<td colspan="2">
    	<h3 style="text-transform:uppercase; font-size:20px"><b><%=data.personaldetails.candidatename%></b></h3>
        <p style="color:#333; font-size:14px">Php Developer</p>
        <div style="font-size:13px; margin:15px 0px !important">
        <p><%=data.personaldetails.permanentaddress%>,<%=data.personaldetails.permanentcity%>, <%=data.personaldetails.permanentstate%></p>
		<p><b>Mobile:&nbsp;</b> +91-<%=data.personaldetails.mobile%></p>  <p><b>Email:&nbsp;</b> <a href="mailto:<%=data.personaldetails.email%>"><%=data.personaldetails.email%></a></p>
		</div>
    </td>
    <td></td>
</tr>

<tr>
	<td style="width:20%; vertical-align:top"><h5><b>Objectives:</b></h5>
    </td>
    <td style="width:80%; font-size:14px">
    <p style="margin-bottom:50px !important"><%=data.cv.objective%></p>
    </td>
</tr>
<tr>
	<td style="width:25%;vertical-align:top	"><h5><b>Work Experience:</b></h5>   </td>
    <td style="width:75%">
    	<div style="margin-bottom:15px !important;">
        	<p><b><%=data.professionaldetails.company.jobtitle%></b></p>
            <p><%=data.professionaldetails.company.companyname%></p>
            <p style="font-size:12px; font-weight:bold">[<%=data.professionaldetails.company.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.professionaldetails.company.enddate%>]</p>
            <p style="margin-top:10px !important"><%=data.professionaldetails.company.roles%></p>
        </div>
       
    </td>
</tr>
<tr>
	<td style="width:20%;vertical-align:top;"><h5 style="margin-top:20px !important"><b>Educational Details:</b></h5>   </td>
    <td style="width:80%">
    	<div style="margin-bottom:15px !important; margin-top:20px !important">
        	<p><b><%=data.educationaldetails.highschool.course%> - [<%=data.educationaldetails.highschool.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.highschool.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</b></p>
            <p><%=data.educationaldetails.highschool.school%></p>
        </div>
        <div style="margin-bottom:15px !important;">
        	<p><b><%=data.educationaldetails.seniorsecondaryschool.course%> -[<%=data.educationaldetails.seniorsecondaryschool.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.seniorsecondaryschool.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</b></p>
            <p><%=data.educationaldetails.seniorsecondaryschool.school%></p>
             
        </div>
         <div style="margin-bottom:15px !important;">
        	<p><b><%=data.educationaldetails.undergraduation.course%> -[<%=data.educationaldetails.undergraduation.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.undergraduation.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</b></p>
            <p><%=data.educationaldetails.undergraduation.university%></p>
             
        </div>
		  <div style="margin-bottom:15px !important;">
        	<p><b><%=data.educationaldetails.postgraduation.course%> -[<%=data.educationaldetails.postgraduation.startdate.toLocaleDateString('de-DE', {year: 'numeric'})%>-<%=data.educationaldetails.postgraduation.enddate.toLocaleDateString('de-DE', {year: 'numeric'})%>]</b></p>
            <p><%=data.educationaldetails.postgraduation.university%></p>
             
        </div>
    </td>
</tr>
<tr>
	<td style="width:20%;vertical-align:top	"><h5 style="margin-top:20px !important"><b>Skills:</b></h5>   </td>
    <td style="width:80%">
    	<div style="margin-bottom:15px !important ;margin-top:20px !important">
        	<p><b>Primary Skill</b></p>
            <p><%=data.skills.primaryskill%></p>
        </div>
        <div style="margin-bottom:15px !important;">
        	<p><b>Secondary Skill</b></p>
            <p><%=data.skills.secondaryskill%></p>
             
        </div>
         
    </td>
</tr>
</table>
</div>
</div>
<%- include footer.ejs%>
<script src="/javascripts/jquery.min.js"></script>
<script src="/javascripts/xeponline.jqplugin.js"></script>
<script>
var click="return xepOnline.Formatter.Format('JSFiddle', {render:'download'})";
jQuery('#buttons').append('<span onclick="'+ click +'" class="btnn" ><img src="data:image/png;base64," width="20px"></span>');
</script>
<style>
table{border-collapse:inherit !important}
.btnn{ background:#000; padding:5px; cursor:pointer ;  right: 416px;
    top: 90px; position:absolute;  }
	
</style>
